	<queryString>
		<![CDATA[SELECT
     program_studi.`id` AS program_studi_id,
     program_studi.`nama` AS program_studi_nama,
     mahasiswa.`id` AS mahasiswa_id,
     mahasiswa.`nama_depan` AS mahasiswa_nama_depan,
     mahasiswa.`nama_belakang` AS mahasiswa_nama_belakang,
     mahasiswa.`tempat_lahir` AS mahasiswa_tempat_lahir,
     mahasiswa.`tanggal_lahir` AS mahasiswa_tanggal_lahir,
     mahasiswa.`alamat` AS mahasiswa_alamat,
     mahasiswa.`email` AS mahasiswa_email,
     mahasiswa.`program_studi_id` AS mahasiswa_program_studi_id
FROM
     `program_studi` program_studi INNER JOIN `mahasiswa` mahasiswa ON program_studi.`id` = mahasiswa.`program_studi_id`
WHERE
	program_studi_id = $P{prodi}]]>
	</queryString>
